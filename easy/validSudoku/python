class Solution:
        def isValidSudoku(self, board: List[List[str]]) -> bool:

            #check all rows
            for row in board:
                #count values:
                r = [val for val in row if val!='.']
                if len(r) != len(set(r)):
                    return False
                
                
            #check all cols:
            for j in range(9):
                col_j = [row[j] for row in board]
                c = [val for val in col_j if val!='.']
                if len(c) != len(set(c)):
                    return False
                
                
            #check boxes:
            
            for box_row in range(3):
                for box_col in range(3):
                    box = [
                        board[r][c]
                        for r in range(3*box_row, 3*box_row+3)
                        for c in range(3*box_col, 3*box_col+3)
                    ]
                    b = [val for val in box if val!='.']
                    if len(b) != len(set(b)):
                        return False
            

            return True
