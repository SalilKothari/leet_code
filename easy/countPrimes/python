class Solution:
    def countPrimes(self, n: int) -> int:
        
        if n < 2:
            return 0
        
        primes = [True] * n
        primes[0] = primes[1] = False
        
        for i in range(2, int(sqrt(n)) + 1):
            if(primes[i]):

                for p in range(i*i, n, i):
                    primes[p] = False
        
        
        return sum(primes)
