class Solution:
    def isPowerOfThree(self, n: int) -> bool:
        
        # threes = [3**x for x in range(0, 20)]
        # return n in threes
        num = n
        
        while num > 2:
            if num%3 !=0:
                return False
            num = num/3
            
        if num == 1:
            return True
        else:
            return False
        
        
