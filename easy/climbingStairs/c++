class Solution {
public:
    int climbStairs(int n) {
        
        size_t prev1 = 1;
        size_t prev2 = 0;
        
        for(size_t i = 0; i<n; ++i){
            size_t current = prev1 + prev2;
            prev2 = prev1;
            prev1 = current;
        }
       
        
        
        return prev1;
        
    
    }
};
