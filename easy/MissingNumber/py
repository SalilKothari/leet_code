class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        
        nums = sorted(nums)
        
        if nums[0] != 0:
            return 0
        
        for num in range(1, len(nums)):
            if nums[num] != nums[num-1] + 1:
                return nums[num -1] + 1 

        
        
        return nums[-1] + 1
