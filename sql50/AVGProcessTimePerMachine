# Better

SELECT 
    machine_id,
    ROUND(SUM(CASE WHEN activity_type = 'start' THEN timestamp * -1 ELSE timestamp
            END) / COUNT(DISTINCT(process_id)),3) AS processing_time
FROM Activity
GROUP BY machine_id;



# My really bad solution
WITH PerMP AS
(
    SELECT 
        a.machine_id, 
        (MAX(a.timestamp) - MIN(a.timestamp)) AS elapsed
    FROM Activity a
    GROUP BY machine_id, process_id
)

SELECT pmp.machine_id, ROUND(AVG(pmp.elapsed), 3) as processing_time
FROM PerMP pmp
GROUP BY pmp.machine_id;
