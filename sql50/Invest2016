# Write your MySQL query statement below
SELECT ROUND(SUM(tiv_2016),2) AS tiv_2016
FROM Insurance i3
WHERE i3.pid IN (
    SELECT DISTINCT i1.pid
    FROM Insurance i1
    LEFT JOIN Insurance i2 ON  i1.pid <> i2.pid
    WHERE i1.tiv_2015 = i2.tiv_2015)
AND (lat, lon) IN(
    SELECT lat, lon
    FROM Insurance i4
    GROUP BY i4.lat, i4.lon
    HAVING COUNT(*) = 1
)
